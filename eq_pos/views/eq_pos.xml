<openerp>
    <data>
    
        <template id="assets_backend" name="eq_pos assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/eq_pos/static/src/js/pos.js"></script>
                <script type="text/javascript" src="/eq_pos/static/src/js/jquery-barcode.js"></script>
            </xpath>
        </template>
        
        <template id="index" name="eq_pos index" inherit_id="point_of_sale.index">
	        <xpath expr="." position="inside">
	        	<link rel="stylesheet" href="/eq_pos/static/src/css/pos.css" id="pos-stylesheet"/>
	        	<link rel="stylesheet" href="/web/static/lib/bootstrap/css/bootstrap.css"/>
	        </xpath>
	    </template>
	    
    </data>
</openerp>