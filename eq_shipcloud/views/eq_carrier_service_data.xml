<?xml version="1.0"?>
<openerp>

       <data noupdate="1">
       
       
       <!-- Carrier - Service for DHL -->
        <record id="eq_dhl_standard" model="eq.shipcloud.service">
            <field name="eq_name">standard</field>
            <field name="eq_shipcloud_carrier">dhl</field>
            <field name="eq_default">True</field>
            <field name="eq_comments">The standard (ground) service for each carrier</field>
        </record>
        
        <record id="eq_dhl_returns" model="eq.shipcloud.service">
            <field name="eq_name">returns</field>
            <field name="eq_shipcloud_carrier">dhl</field>
            <field name="eq_comments">Returns service</field>
        </record>
        
        <record id="eq_dhl_one_day" model="eq.shipcloud.service">
            <field name="eq_name">one_day</field>
            <field name="eq_shipcloud_carrier">dhl</field>
            <field name="eq_comments">express delivery</field>
        </record>
        
        <record id="eq_dhl_one_day_early" model="eq.shipcloud.service">
            <field name="eq_name">one_day_early</field>
            <field name="eq_shipcloud_carrier">dhl</field>
            <field name="eq_comments">express delivery until 10am.</field>
        </record>
        
        
